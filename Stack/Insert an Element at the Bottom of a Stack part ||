stack<int> insertAtBottom(stack<int> &st,int x){
        
        // base case
        if(st.empty())
            {
                st.push(x);
                return st;
            }
            
        // recyrsive condision
        int a =st.top();
                st.pop();
        insertAtBottom(st,x);
        
        st.push(a);
        
        return st;
}
