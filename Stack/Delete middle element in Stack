
class Solution
{
    public:
    //Function to delete middle element of a stack.
    void deleteMid(stack<int>&s, int sizeOfStack)
    {
        // code here.. 
        int len=sizeOfStack;
        int mid=len/2;
        int i=0;
        vector<int >v;
        while(i<mid)
        {
          v.push_back(s.top())  ;
          s.pop();
          i++;
        }
        s.pop();
        i--;
        while(i>-1)
        {
            s.push(v[i]);
            i--;
        }
    }
};
